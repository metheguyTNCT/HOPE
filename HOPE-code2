    function addGovernmentButton() {
        if (document.getElementById('government_button')) {
            return;
        }
    
        const refButton = document.getElementById('view_electoral_map');
        if (!refButton) {
            return;
        }
    
        const newButton = document.createElement('button');
        newButton.id = 'government_button';
        newButton.innerHTML = 'Government';
        newButton.addEventListener('click', openRelationsView);
        newButton.style.marginLeft = "25px";
        refButton.insertAdjacentElement('afterend', newButton);
    }
    
    // Function to open the Administration Relations View
    function openRelationsView() {
        newRelationsData = false; // Reset new data indicator
        document.getElementById('government_button').innerHTML = 'Government';
        let questions = document.querySelector(".inner_window_question");
        questions.style.display = 'none';
    
        let relationsDiv = document.createElement('div');
        relationsDiv.id = 'relations_view';
        relationsDiv.style.height = '100%';
        setupRelationsArea(relationsDiv, questions);
    
        questions.parentNode.insertBefore(relationsDiv, questions.nextSibling);
    }
    
    function setupRelationsArea(parentDiv, toggleDiv) {
      let current_page = 0; // Start on page 0
  
      const page_list = [
          ["Congressional Majority:", "error", "Congressional Approval:", "error",],
          ["Public Support:", "error", "Legacy:", "error"],
          ["Foreign Reputation:", "error", "Relationship with Hillary:", "error"],
          ["Expected Opponent:", "Unknown"]
      ];
  
  function styleSpecialWords() {
      // Find special words and wrap them in a styled span tag
      page_list.forEach((page, pageIndex) => {
          page_list[pageIndex] = page.map(content => {
              if (content === "Unknown") {
                  return `<span style="font-weight: bold; color: red; font-size: 24px;">${content}</span>`;
              }
              return content; // Leave other content unchanged
          });
      });
  }
  
  styleSpecialWords(); // Apply styles to the list
  
   // Function to update page_list based on conditions
      function updatePageList() {
       ans = campaignTrail_temp.player_answers[campaignTrail_temp.player_answers.length - 1]
      if (DemCongress === 1) {
          page_list[0][1] = `<span style="font-weight: bold; color: blue; font-size: 24px;">SLIM DEM MAJORITY</span>`; // Style DEM
          page_list[0][3] = `<span style="font-weight: bold; color: gray; font-size: 24px;"> Average</span>`; // Style Congressional Approval
      } else if (RepublicanRevolution == 1) {
          page_list[0][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> HUGE GOP MAJORITY</span>`; // Style GOP
          page_list[0][3] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Horrible</span>`; // Style Congressional Approval
      } else if (SlimGOPCongress == 1) {
          page_list[0][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> SLIM GOP MAJORITY</span>`; // Style GOP
          page_list[0][3] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Bad</span>`; // Style Congressional Approval
      } else if (GOPSupermajority == 1) {
          page_list[0][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> GOP SUPERMAJORITY</span>`; // Style GOP
          page_list[0][3] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Abysmal</span>`; // Style Congressional Approval
      } if (Legacy === 0) {
          page_list[1][3] = `<span style="font-weight: bold; color: black; font-size: 24px;"> None</span>`; // Style Legacy
      } else if (Legacy >= 1 && Legacy <= 2) {
          page_list[1][3] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Low</span>`; // Style Legacy
        } else if (Legacy == 3) {
          page_list[1][3] = `<span style="font-weight: bold; color: teal; font-size: 24px;"> Decent</span>`; // Style Legacy
      } else if (Legacy >= 4 && Legacy <= 5) {
          page_list[1][3] = `<span style="font-weight: bold; color: blue; font-size: 24px;"> Great</span>`; // Style Legacy
      } else if (Legacy === 6) {
          page_list[1][3] = `<span style="font-weight: bold; color: gold; font-size: 24px;"> Perfect</span>`; // Style Legacy
      } if (PublicSupport <= 0) {
          page_list[1][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Abysmal</span>`; // Style PublicSupport
        } else if (PublicSupport >= 1 && PublicSupport <= 5) {
          page_list[1][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Low</span>`; // Style PublicSupport
        } else if (PublicSupport >= 6 && PublicSupport <= 10) {
          page_list[1][1] = `<span style="font-weight: bold; color: gray; font-size: 24px;"> Medium</span>`; // Style PublicSupport
        } else if (PublicSupport >= 11 && PublicSupport <= 15) {
          page_list[1][1] = `<span style="font-weight: bold; color: gray; font-size: 24px;"> High</span>`; // Style PublicSupport
        } else if (PublicSupport >= 16 && PublicSupport <= 20) {
          page_list[1][1] = `<span style="font-weight: bold; color: gray; font-size: 24px;"> Very High</span>`; // Style PublicSupport
      } if (FPWin <= 0) {
          page_list[2][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Laughingstock</span>`; // Style ForeignReputation
      } else if (FPWin >= 1 && FPWin <=3) {
          page_list[2][1] = `<span style="font-weight: bold; color: green; font-size: 24px;"> Respected</span>`; // Style ForeignReputation
      } else if (FPWin >= 4 && FPWin <=5) {
          page_list[2][1] = `<span style="font-weight: bold; color: blue; font-size: 24px;"> Honoured</span>`; // Style ForeignReputation
      }    if (HillaryRelationship === 4) {
          page_list[2][3] = `<span style="font-weight: bold; color: blue; font-size: 24px;"> Ok</span>`; // Style HillaryRelationship
      } else if (HillaryRelationship >= 5 && HillaryRelationship <= 7) {
          page_list[2][3] = `<span style="font-weight: bold; color: blue; font-size: 24px;"> Good</span>`; // Style HillaryRelationship
      } else if (HillaryRelationship == 8) {
          page_list[2][3] = `<span style="font-weight: bold; color: blue; font-size: 24px;"> Great</span>`; // Style HillaryRelationship
      } if (ans == 3425) {
          page_list[3][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Steve Forbes</span>`; // Style ExpectedOpponent
      } else if (ans == 3426) {
          page_list[3][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Steve Forbes</span>`; // Style ExpectedOpponent
      } if (LibSocial == 2 && GOPSupermajority == 1 && ans == 3365) {
          page_list[3][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Pat Buchanan</span>`; // Style ExpectedOpponent
      } else if (LibSocial == 2 && GOPSupermajority == 1 && ans == 12631) {
          page_list[3][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Pat Buchanan</span>`; // Style ExpectedOpponent
        } if (LibSocial == 3 && GOPSupermajority == 1 && ans == 3365) {
          page_list[3][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Lamar Alexander</span>`; // Style ExpectedOpponent
      } else if (LibSocial == 3 && GOPSupermajority == 1 && ans == 12631) {
          page_list[3][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Lamar Alexander</span>`; // Style ExpectedOpponent
      } else if (RepublicanRevolution == 1 && ans == 3365){ 
          page_list[3][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Bob Dole</span>`; // Style ExpectedOpponent
      } else if (LibSocial >= 1 && RepublicanRevolution == 1 && ans == 12631){ 
          page_list[3][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Bob Dole</span>`; // Style ExpectedOpponent
      } else if (SlimGOPCongress == 1 && ans == 3365){ 
          page_list[3][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Bob Dole</span>`; // Style ExpectedOpponent
      } else if (SlimGOPCongress == 1 && ans == 12631){ 
          page_list[3][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Bob Dole</span>`; // Style ExpectedOpponent
      }    else if (LibSocial <= 1 && GOPSupermajority == 1 && ans == 3365){ 
          page_list[3][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Bob Dole</span>`; // Style ExpectedOpponent
      } else if (LibSocial <= 1 && GOPSupermajority == 1 && ans == 12631){ 
          page_list[3][1] = `<span style="font-weight: bold; color: red; font-size: 24px;"> Bob Dole</span>`; // Style ExpectedOpponent
      }
  }
  
      // Update page_list before rendering
      updatePageList();
  
  
      // Create the header
      let headerDiv = createHeader(parentDiv, toggleDiv, page_list);
      parentDiv.appendChild(headerDiv);
  
      // Create the container for the page content
      const textContainer = document.createElement('div');
      textContainer.style.width = '80%';
      textContainer.style.height = '300px';
      textContainer.style.margin = '20px auto';
      textContainer.style.border = '1px solid lightgray';
      textContainer.style.borderRadius = '12px';
      textContainer.style.backgroundColor = 'white';
      textContainer.style.display = 'flex';
      textContainer.style.flexDirection = 'column';
      textContainer.style.justifyContent = 'center'; // Center content vertically
      textContainer.style.alignItems = 'center'; // Center content horizontally
      textContainer.style.padding = '20px';
      textContainer.style.boxShadow = '0 4px 8px rgba(0, 0, 0, 0.1)';
      parentDiv.appendChild(textContainer);
  
      // Function to render content for the current page
      function renderPage(pageIndex) {
          textContainer.innerHTML = ''; // Clear previous content
          const pageContent = page_list[pageIndex];
  
          pageContent.forEach(content => {
              const paragraph = document.createElement('p');
              paragraph.innerHTML = content;
              paragraph.style.fontSize = '20px';
              paragraph.style.margin = '10px 0';
              paragraph.style.textAlign = 'center'; // Center align text
              textContainer.appendChild(paragraph);
          });
      }
  
      // Initial page render
      renderPage(current_page);
  
      // Function to create the header
      function createHeader(parentDiv, toggleDiv, page_list) {
          let headerDiv = document.createElement('div');
          headerDiv.style.display = 'flex';
          headerDiv.style.color = 'black';
          headerDiv.style.justifyContent = 'space-between';
          headerDiv.style.alignItems = 'center';
          headerDiv.style.backgroundColor = '#d8dde6';
          headerDiv.style.borderRadius = '12px';
          headerDiv.style.padding = '12px 12px';
          headerDiv.style.width = '250px';
          headerDiv.style.maxWidth = '100%';
          headerDiv.style.margin = '0 auto';
  
          headerDiv.appendChild(createBackButton(parentDiv, toggleDiv));
          headerDiv.appendChild(createHeaderText('Government'));
          headerDiv.appendChild(createNextButton());
          return headerDiv;
      }
  
      // Create a back button to exit
      function createBackButton(parentDiv, toggleDiv) {
          let backButton = document.createElement('button');
          backButton.innerText = 'Back';
          backButton.style.margin = '5px';
          backButton.style.backgroundColor = '#223b61';
          backButton.style.color = '#fff';
          backButton.onclick = function () {
              parentDiv.remove();
              toggleDiv.style.display = '';
          };
          return backButton;
      }
  
      // Create a next button to navigate pages
      function createNextButton() {
          let nextButton = document.createElement('button');
          nextButton.innerText = 'Next';
          nextButton.style.margin = '5px';
          nextButton.style.backgroundColor = '#223b61';
          nextButton.style.color = '#fff';
          nextButton.style.cursor = 'pointer';
  
          // Handle navigation to the next page
          nextButton.addEventListener('click', () => {
              current_page = (current_page + 1) % page_list.length; // Cycle through pages
              renderPage(current_page);
          });
  
          return nextButton;
      }
  
      // Create the header title
      function createHeaderText(title) {
          const headerTitle = document.createElement('h3');
          headerTitle.innerText = title;
          headerTitle.style.margin = '0';
          return headerTitle;
      }
  }
  
  // Mutation observer setup
  let observerRunning = false;
  let element = document.getElementById('controlElement');
  if (!element) {
      let singleObserver = new MutationObserver(handleMutations);
      singleObserver.observe(document.documentElement, { childList: true, subtree: true });
      let controlElement = document.createElement('div');
      controlElement.style.display = 'none';
      controlElement.id = 'controlElement';
      document.body.appendChild(controlElement);
  }
  
  // Handle DOM mutations to add the button in case the target is not present
  function handleMutations(mutationsList, observer) {
      if (observerRunning) return;
      observerRunning = true;
      addGovernmentButton();
      observer.observe(document.documentElement, { childList: true, subtree: true });
      observerRunning = false;
  }
  
  // Function to refresh relations view
  function refreshRelationsView() {
      let relationsDiv = document.getElementById('relations_view');
      if (relationsDiv) {
          relationsDiv.innerHTML = ''; // Clear the existing content
          setupRelationsArea(relationsDiv, document.querySelector(".inner_window_question"));
      }
  }
  

ev_dict={};
pop_dict={};

  function update_EV_dict() {
    let pop_vote = e.current_results;
    ev_dict={};
    pop_dict={};
    for (let n = 0; n<e.current_results[0].length; n++) {
        ev_dict[e.current_results[0][n].pk] = e.current_results[0][n].evvs;
        pop_dict[e.current_results[0][n].pk] = e.current_results[0][n].pvp;
    }
}
// Access the updated dictionaries
console.log(ev_dict); // Logs the ev_dict object
  // Since you're going to be doing a lot of inserting, you don't want to be using exact positions. 
  // Position 8 could be any number of questions depending on previous CYOA. 
  // This function goes by pk values.
  // Simply put in the pk you want as a parameter, and it will insert the question AFTER that pk.
  // For example, to insert a question "test" after the pk 9999, use question_insert(9999, "test");
  
  function question_insert(insertafter_pk,question) {
      let foundpk = false;
      for (x in campaignTrail_temp.questions_json) {
          if (campaignTrail_temp.questions_json[x]["pk"] == insertafter_pk && foundpk == false) {
              campaignTrail_temp.questions_json.splice(Number(x)+1,0,question);
              foundpk = true;
          }
      }
      if (foundpk == false) {
          console.log("PK not found! "+insertafter_pk);
      }
  }
